FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    transport
    {
        mu          0;
        Pr          1;
    }
    thermodynamics
    {
        Cp          6000;
        Hf          0;
    }
    equationOfState
    {
        rho         2500;
    }
    specie
    {
        molWeight   100;
    }
}



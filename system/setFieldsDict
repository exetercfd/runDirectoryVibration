FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.particles 0
);

regions
(
    boxToCell
    {
        box ( 0 0 -0.2 ) ( 0.4 0.02 0.2 );
        fieldValues
        (
            volScalarFieldValue alpha.particles 0.6
        );
    }    
    
);


